  "scripts": {
    "ng": "ng",
    "e2e": "ng e2e",
    "test": "ng test",
    "lint": "ng lint",
    "start": "npm run local",
    "local": "ng serve --open",
    "release": "standard-version",
    "build": "ng build --prod --build-optimizer",
